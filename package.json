{
  "name": "commerical_web",
  "version": "1.0.0",
  "main": "dist/app.js",
  "scripts": {
    "start": "npm run build && node dist/app.js",
    "lint": "eslint . --ext .ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "npx nodemon",
    "build": "node swagger.js && tsc",
    "vercel-build": "npm run build",
    "swagger-autogen": "node swagger.js",
    "swagger-jsdoc": "node swagger-jsdoc.js",
    "deploy": "node --max_old_space_size=4096 dist/app.js --host 0.0.0.0"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts,js,json",
    "ignore": [
      "dist",
      "node_modules"
    ],
    "exec": "npm run start",
    "delay": "2500"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@eslint/js": "^9.26.0",
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.18",
    "@types/express": "^4.17.1",
    "@types/jsonwebtoken": "^9.0.9",
    "eslint": "^9.26.0",
    "globals": "^16.0.0",
    "swagger-autogen": "^2.23.7",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.32.0"
  },
  "dependencies": {
    "@types/multer": "^1.4.13",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "bcrypt": "^6.0.0",
    "cloudinary": "^2.6.1",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.21.2",
    "google-auth-library": "^10.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.14.2",
    "multer": "^2.0.1",
    "serverless-http": "^3.2.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1"
  }
}
